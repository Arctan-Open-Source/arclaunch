<?xml version="1.0"?>
<launch name="stdout_launch" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://matt5sean3.com/launch.xsd">
  <node name="stdout" xsi:type="script_t">
    <path absolute="false" os="linux">
      <directory>scripts</directory>
      <directory>main.py</directory>
    </path>
    <interpreter name="python_interpreter">
      <path absolute="true" os="linux">
        <directory>usr</directory>
        <directory>bin</directory>
        <directory>python</directory>
      </path>
    </interpreter>
  </node>
</launch>
