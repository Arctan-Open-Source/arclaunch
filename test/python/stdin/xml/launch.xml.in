<?xml version="1.0"?>
<launch name="stdin_launch" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="@LAUNCH_XML_SCHEMA@">
  <node name="stdin" xsi:type="script_t">
    <path os="linux" absolute="false">
      <directory>scripts</directory>
      <directory>main.py</directory>
    </path>
    <interpreter name="python_interpreter">
      <path os="linux" absolute="true">
        <directory>usr</directory>
        <directory>bin</directory>
        <directory>python</directory>
      </path>
    </interpreter>
  </node>
</launch>
