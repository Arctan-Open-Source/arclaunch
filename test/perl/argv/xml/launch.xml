<?xml version="1.0"?>
<launch name="argv_launch" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://matt5sean3.com/launch.xsd">
  <node name="argv" xsi:type="script_t">
    <path absolute="false" os="linux">
      <directory>scripts</directory>
      <directory>main.pl</directory>
    </path>
    <arg>Perl Script</arg>
    <interpreter name="perl_interpreter">
      <path absolute="true" os="linux">
        <directory>usr</directory>
        <directory>bin</directory>
        <directory>perl</directory>
      </path>
    </interpreter>
  </node>
</launch>
