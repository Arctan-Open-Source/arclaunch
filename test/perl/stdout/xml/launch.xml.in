<?xml version="1.0"?>
<launch name="stdout_launch" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="@LAUNCH_XML_SCHEMA@">
  <node name="stdout" xsi:type="script_t">
    <path os="linux" absolute="false">
      <directory>scripts</directory>
      <directory>main.pl</directory>
    </path>
    <interpreter name="perl_interpreter">
      <path os="linux" absolute="true">
        <directory>usr</directory>
        <directory>bin</directory>
        <directory>perl</directory>
      </path>
    </interpreter>
  </node>
</launch>
