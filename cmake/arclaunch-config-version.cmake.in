set(PACKAGE_VERSION_MAJOR "@ARCLAUNCH_VERSION_MAJOR@")
set(PACKAGE_VERSION_MINOR "@ARCLAUNCH_VERSION_MINOR@")
set(PACKAGE_VERSION_PATCH "@ARCLAUNCH_VERSION_PATCH@")
set(PACKAGE_VERSION "@ARCLAUNCH_VERSION_MAJOR@.@ARCLAUNCH_VERSION_MINOR@.@ARCLAUNCH_VERSION_PATCH@")

# Major versions must be equal
# Minor version must be greater or equal
# Patch version has no bearing except exact matching
if("${PACKAGE_VERSION_MAJOR}" EQUAL "${PACKAGE_FIND_VERSION_MAJOR}" AND NOT "${PACKAGE_VERSION_MINOR}" LESS "${PACKAGE_FIND_VERSION_MINOR}")
  set(PACKAGE_VERION_COMPATIBLE TRUE)
  if("${PACKAGE_VERSION}" VERSION_EQUAL "${PACKAGE_FIND_VERSION}")
    set(PACKAGE_VERSION_EXACT TRUE)
  endif("${PACKAGE_VERSION}" VERSION_EQUAL "${PACKAGE_FIND_VERSION}")
else("${PACKAGE_VERSION_MAJOR}" EQUAL "${PACKAGE_FIND_VERSION_MAJOR}" AND NOT "${PACKAGE_VERSION_MINOR}" LESS "${PACKAGE_FIND_VERSION_MINOR}")
  set(PACKAGE_VERION_COMPATIBLE FALSE)
endif("${PACKAGE_VERSION_MAJOR}" EQUAL "${PACKAGE_FIND_VERSION_MAJOR}" AND NOT "${PACKAGE_VERSION_MINOR}" LESS "${PACKAGE_FIND_VERSION_MINOR}")
